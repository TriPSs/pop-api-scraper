{
  "name": "@pct-org/pop-api-scraper",
  "version": "0.8.1",
  "description": "The base modules for the popcorn-api scraper",
  "keywords": [
    "api",
    "middleware",
    "popcorn",
    "scraper"
  ],
  "license": "MIT",
  "files": [
    "build/**/*"
  ],
  "main": "./build/pop-api-scraper.js",
  "module": "./build/pop-api-scraper.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pct-org/pop-api-scraper.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "debug": "npm run dev --inspect",
    "docs": "esdoc -c .esdoc.json",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint": "eslint {example,src,test}/** *.js$",
    "prepublish": "yarn build",
    "start:dev": "cross-env DEBUG=pop-api-scraper* NODE_ENV=development node -r babel-register ./examples/exampleSetup.js",
    "test": "cross-env NODE_ENV=test nyc -r=lcov -r=text mocha --exit --recursive"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "debug": "4.1.1",
    "fs-extra": "8.1.0",
    "got": "9.6.0",
    "node-cron": "^2.0.3",
    "p-map": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-plugin-istanbul": "5.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "del": "5.1.0",
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "6.3.0",
    "eslint-config-vixo": "^2.0.2",
    "flow": "^0.2.3",
    "flow-bin": "0.107.0",
    "flow-typed": "^2.2.3",
    "husky": "^3.0.5",
    "mkdirp": "^0.5.1",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "rollup": "1.21.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-uglify": "6.0.3",
    "sinon": "7.4.2",
    "standard-version": "7.0.0",
    "uglify-es": "^3.3.4"
  },
  "peerDependencies": {
    "pop-api": "^0.6.1"
  },
  "engines": {
    "node": ">=7.10.1"
  }
}